{
  "defaultSeverity": "error",
  "extends": [
    "tslint:recommended"
  ],
  "jsRules": {},
  "rules": {
    "semicolon": [
      true,
      "never"
    ],
    "no-console": [
      false
    ],
    "interface-name": [
      false
    ],
    "quotemark": [
      true,
      "single",
      "jsx-double"
    ],
    "ordered-imports": false,
    "object-literal-sort-keys": false,
    "arrow-parens": false
  },
  "rulesDirectory": []
}